{
  "extends": [
    "tslint:latest"
  ],
  "rules": {
    "no-new": 0,
    "trailing-comma": [
      false,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "indent": [
      true,
      "spaces"
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "no-var-requires": false,
    "ordered-imports": false,
    "member-ordering": [
      false
    ],
    "object-literal-sort-keys": false,
    "no-shadowed-variable": false,
    "no-console": [
      false
    ],
    "jsx-alignment": [
      false
    ]
  }
}
